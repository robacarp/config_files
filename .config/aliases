alias l="\ls"
alias g="git"
alias gs="git status"
alias pull="git pull"
alias push="git push"
alias check="git checkout"
alias servers="grep -e \"Host \"  ~/.ssh/config"
alias tpl="terraform plan -out=plan.out | landscape"
alias tao="terraform apply plan.out"

