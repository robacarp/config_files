alias l="\ls"
alias md="mkdir"
alias r="rails"
alias rs="rails server --debug"
alias rc="rails console --debug"
alias ack="ack-grep"
alias g="git"
alias gs="git status"
alias pull="git pull"
alias push="git push"
alias check="git checkout"
alias irc="irssi"
alias go="ssh"
alias cld="cd \`\ls -t | head -n1\`"  #cd into the directory with the newest timestamp
alias axel="axel -a"
alias laxel="axel -s 2097152"

alias redis="redis-server /usr/local/etc/redis.conf"
alias elastic="elasticsearch -f -D es.config=/usr/local/opt/elasticsearch/config/elasticsearch.yml"
